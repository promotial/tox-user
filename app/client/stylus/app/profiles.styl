/* PROFILES LIST */

//profiles list container
#profiles-list
  position absolute
  height auto
  width 100%
  top 55px
  bottom 55px
  background-color white
  overflow auto
//profile item class
.profile-item
  width 100%
  height 50px
  border-bottom 1px solid #666666
//profile-name class
.profile-name
  font-size 24px
  margin-left 20px
  line-height 55px
//profile-edit class
.profile-edit
  font-size 30px
  float right
  height 50px
  margin-right 15px
  line-height 58px
//profile-delete class
.profile-delete
  font-size 30px
  float right
  height 50px
  margin-right 20px
  line-height 54px
/* --PROFILES LIST */
